                  <m:mrow/>
                </m:mrow>
                <m:annotation encoding="StarMath 5.0"> size 12{n rSub { size 8{ ital "HCl"` ital "neutralized"` ital "by"` ital "Antacid"} } =n rSub { size 8{ ital "HCl"` ital "added"} }  - n rSub { size 8{ ital "HCl"` ital "excess"} } } {}</m:annotation>
