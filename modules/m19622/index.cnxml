                                    </m:mrow>
                                  </m:mstyle>
                                  <m:mi/>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>excess</m:mtext>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:mrow>
                              </m:mrow>
                            </m:mstyle>
                          </m:msub>
                        </m:mrow>
                      </m:mrow>
                    </m:mrow>
